# Tehtävä 12

def vowels(string):
	count = 0 
	vowels = 'a' or 'e' or 'i' or 'o' or 'u'
#	vowels = 'aeiouAEIOU'
#	vowels = set("aeiou")
	for letter in string:
		if letter in vowels:
			count += 1
	return count

print(vowels("aaa")) # prints: 3
print(vowels("aba")) # prints: 2
print(vowels("bca")) # prints: 1

